<h1>Create a Song</h1>

<%= form_for @song  do |f| %>
  <%= f.label :title %>
  <%= f.text_field :title %>
  <%= f.label :artist_name %>
  <%= f.text_field :artist_name %>
  <%= f.text_field :genre_id, list: "genres_autocomplete" %>
  <datalist id="genres_autocomplete">
    <% Genre.all.each do |g| %>
      <option value="<%= g.name %>">
    <% end %>
  </datalist>
  <input id="song_notes_attributes_0_content" name="song[notes][]">
  <input id="song_notes_attributes_1_content" name="song[notes][]">
  <br />
  <%= f.submit %>
<% end %>


