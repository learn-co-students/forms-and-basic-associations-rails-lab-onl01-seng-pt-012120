<h1>Create a Song</h1>

<%= form_for @song  do |f| %>

  <div>
  <%= f.label :title %>
  <%= f.text_field :title %>
  <br />
  </div>

  <div>
  <%= f.label :artist_name %>
  <%= f.text_field :artist_name %>
  <br />
  </div>

  <div>
  <%= f.collection_select(:genre_id, Genre.all, :name, :name) %>
  <%= f.collection_select :name %>
  <br />
  </div>

  <%= f.submit %>
<% end %>
